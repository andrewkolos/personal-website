{
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "declaration": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
